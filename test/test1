

ns test1.fish  --

import data.text ;
import data.text as t ;
open control.monad ;
from control import applicative


unit = () ;
t = true ;
f = false ;
d = 2.0 ;
i = 2 ;
c = 'a' ;
s = "hellow world\n" ;
r = r"hello world\n" ;  -- bad
sym = :sym ;
var = com.amazon.orca.auth ;

error = ;

op = 1 :+: 2 ;
opsec = (++ 2) ;
opsec = (3 ++) ;

match-st = // true { :xx => true } ;
match-lambda = \\ { true => 1 } ;
match-lambda-effects = \\ io, state { true => 1 } ;

defmacro = \% a, b -> a + b ;
binary-search = \ bst, x -> {
    true;
    -- world
    false;
    -| hello
    this world |-
    1
} ;

try-catch = %\ bst, x -> 
    try:     { %32 ;
               throw: $xx } 
    catch:   { true  => x - 2;
               false => 0 }
    finally: { close f } ;

list-comp = +{ 
    x <- a; 
    y <- b; 
    filterM (f a b); 
    filterM (a < b); 
    (a,b) 
}



