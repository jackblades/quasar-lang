

defn markov-data [text] =
    let :sentences (~@@~`clojure.string/split \text #"\\.'a'" "hello")
        :maps      (mapcat make-markov-map sentences #[x y]filter)
        :in        apply merge-with concat maps #{ -2 } #filter
        $        apply merge-with concat maps #{ -2 } #filter
          






